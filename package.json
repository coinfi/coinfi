  "scripts": {
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "precommit": "lint-staged",
    "build-webpack:dev:client": "PROJECT_PATH=. NODE_ENV=development webpack --config ./client/config/webpack.client.rails.build.config.js",
    "build-webpack:dev:server": "PROJECT_PATH=. NODE_ENV=development webpack --config ./client/config/webpack.server.rails.build.config.js",
    "build-webpack:production": "rm -rf public/webpack/production && yarn run build-webpack:production:client && yarn run build-webpack:production:server",
    "build-webpack:production:client": "PROJECT_PATH=. NODE_ENV=production webpack -p --config ./client/config/webpack.client.rails.build.config.js",
    "build-webpack:production:server": "PROJECT_PATH=. NODE_ENV=production webpack -p --config ./client/config/webpack.server.rails.build.config.js",
    "build-webpack:test": "rm -rf public/webpack/test && yarn run build-webpack:test:client && yarn run build-webpack:test:server",
    "build-webpack:test:client": "PROJECT_PATH=. NODE_ENV=test webpack --config ./client/config/webpack.client.rails.build.config.js",
    "build-webpack:test:server": "PROJECT_PATH=. NODE_ENV=test webpack --config ./client/config/webpack.server.rails.build.config.js",
    "watch-webpack:dev:client": "yarn run build-webpack:dev:client -w",
    "watch-webpack:dev:client-hot": "PROJECT_PATH=. NODE_ENV=development babel-node ./client/server-rails-hot.js",
    "watch-webpack:dev:server": "yarn run build-webpack:dev:server -w",
    "serve-webpack": "foreman start -f Procfile.static -m webpack-client=1,webpack-server=1",
    "serve": "foreman start -f Procfile.static",
    "test": "jest",
    "test-watch": "jest --watch"
