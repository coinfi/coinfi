{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": "> 1%",
          "uglify": true
        },
        "useBuiltIns": true
      }
    ],
    "react"
  ],
  "plugins": [
    "babel-plugin-styled-components",
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    [
      "babel-plugin-root-import",
      {
        "rootPathSuffix": "./app/javascript/App/",
        "rootPathPrefix": "~"
      }
    ],
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ]
  ]
  
}
